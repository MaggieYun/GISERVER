#coding=utf-8
class Extent:
    def __init__(self,bbox):
        self.bbox = bbox
        self.xmin = bbox[0]
        self.ymin = bbox[1]
        self.xmax = bbox[2]
        self.ymax = bbox[3]        

    def is_within_bbox(self,point):  
        "@point 2-list:[x,y]"
        if ((self.xmin<=point[0]<=self.xmax) and (self.ymin<=point[1]<=self.ymax)):
            return True
        else:
            return False