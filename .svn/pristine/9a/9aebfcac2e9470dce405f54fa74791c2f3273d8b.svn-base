<!-- <Map  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs"> -->
<Map  srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +wktext +over +no_defs">
  <Style name="Style1">
    <Rule>
      <Filter>[roadtype] = 'hw_ramp'</Filter>
      <LineSymbolizer stroke="red" stroke-width="0.5" />
    </Rule>
    <Rule>
      <Filter>[roadtype] = 'main_road'</Filter>
      <LineSymbolizer stroke="#122F7F" stroke-width="4" />
    </Rule>

    <Rule>
      <ElseFilter/>
      <LineSymbolizer stroke="black" stroke-width="2" />
      <TextSymbolizer size="10" fill="white" face-name="Microsoft YaHei Regular" halo-fill="#2E2F39" halo-radius="1" wrap-width="20">[roadname]</TextSymbolizer>
    </Rule>
  </Style>

  <Layer name="PostGIS Layer" status="on"  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>Style1</StyleName>
<!--       <Datasource>
        <Parameter name="type">postgis</Parameter>
        <Parameter name="host">localhost</Parameter>
        <Parameter name="dbname">template_postgis_20</Parameter>
        <Parameter name="user">postgres</Parameter>      
        <Parameter name="password">123456</Parameter>
        <Parameter name="table">viwpt</Parameter>
        <Parameter name="estimate_extent">false</Parameter>
        <Parameter name="extent">-180,-90,180,89.99</Parameter>
      </Datasource> -->
  </Layer>

</Map>