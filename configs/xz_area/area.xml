<Map  srs="+proj=longlat +datum=WGS84 +no_defs">
<!-- <Map  srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +wktext +over +no_defs"> -->
  <Style name="Style">
    <Rule>
      <Filter>[name] = '云龙区' or [name] = '鼓楼区' or [name] = '铜山区' or [name] = '贾汪区' or [name] = '泉山区'</Filter>
      <PolygonSymbolizer fill-opacity="0.05" fill="green" gamma=".65"/>
      <TextSymbolizer size="10" fill="white" face-name="Microsoft YaHei Regular" halo-fill="#2E2F39" halo-radius="1" wrap-width="20" placement="line">[name]</TextSymbolizer>
    </Rule>

    <Rule>
      <ElseFilter/>
      <PolygonSymbolizer fill-opacity="0.05" fill="blue" gamma=".65"/>
      <TextSymbolizer size="10" fill="white" face-name="Microsoft YaHei Regular" halo-fill="#2E2F39" halo-radius="1" wrap-width="20" placement="line">[name]</TextSymbolizer>
    </Rule>
  </Style>

  <Layer name="xuzhouArea" status="on" srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +wktext +over +no_defs">
      <StyleName>Style</StyleName>
  </Layer>

</Map>