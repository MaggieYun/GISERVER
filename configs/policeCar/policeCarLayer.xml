<Map srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
  <Style name="My Style">
    <Rule>
      <MaxScaleDenominator>139770</MaxScaleDenominator>
      <Filter>[TYPE] = "1" and [STATUS] = 1</Filter>
      <PointSymbolizer file="../../resources/images/car_g.png" width="8" height="8" type="png" />
    </Rule>
    <Rule>
      <MaxScaleDenominator>139770</MaxScaleDenominator>
      <Filter>[TYPE] = "1" and [STATUS] = 0</Filter>
      <PointSymbolizer file="../../resources/images/car_r.png" width="8" height="8" type="png" />
    </Rule>
    <Rule>
      <MaxScaleDenominator>139770</MaxScaleDenominator>
      <Filter>[TYPE] = "1" and [STATUS] = 2</Filter>
      <PointSymbolizer file="../../resources/images/car_y.png" width="8" height="8" type="png" />
    </Rule>
    <Rule>
      <MaxScaleDenominator>139770</MaxScaleDenominator>
      <Filter>[TYPE] = "2" and [STATUS] = 1</Filter>
      <PointSymbolizer file="../../resources/images/cycle_g.png" width="8" height="8" type="png" />
    </Rule>
    <Rule>
      <MaxScaleDenominator>139770</MaxScaleDenominator>
      <Filter>[TYPE] = "2" and [STATUS] = 0</Filter>
      <PointSymbolizer file="../../resources/images/cycle_r.png" width="8" height="8" type="png" />
    </Rule>
    <Rule>
      <MaxScaleDenominator>139770</MaxScaleDenominator>
      <Filter>[TYPE] = "2" and [STATUS] = 2</Filter>
      <PointSymbolizer file="../../resources/images/cycle_y.png" width="8" height="8" type="png" />
    </Rule>
  </Style>

  <Layer name="My Layer" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>My Style</StyleName>
  </Layer>

</Map>