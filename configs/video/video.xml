<Map srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
  <Style name="My Style">
    <Rule>
      <Filter>[STATUS] = '1'</Filter>
      <PointSymbolizer file="../../resources/images/map_icons/device/sp.png" width="8" height="8" type="png" />
    </Rule>
    <Rule>
      <ElseFilter/>
      <PointSymbolizer file="../../resources/images/map_icons/device/sp_3.png" width="8" height="8" type="png" />
    </Rule>
  </Style>

  <Layer name="My Layer" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>My Style</StyleName>
  </Layer>
</Map>